{
  "name": "open-source-lecture-slides",
  "version": "1.0.0",
  "description": "",
  "main": "reveal-md path/to/my/slides.md",
  "scripts": {
    "start": "reveal-md slides.md --theme theme/qt-dark.css",
    "start-dark": "reveal-md slides.md --theme theme/qt-dark.css",
    "start-light": "reveal-md slides.md --theme theme/qt-light.css",
    "pdf": "(killall -9 node || echo 'Process was not running.') && reveal-md slides.md --theme theme/qt-light.css --print slides.pdf",
    "dev": "(killall -9 node || echo 'Process was not running.') && reveal-md slides.md -w --theme theme/qt-dark.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/quitequinn/Open-Source-Lecture-Slides.git"
  },
  "author": "Quinn Keaveney",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/quitequinn/Open-Source-Lecture-Slides/issues"
  },
  "homepage": "https://github.com/quitequinn/Open-Source-Lecture-Slides#readme",
  "dependencies": {
    "reveal-md": "^3.4.7"
  }
}
